<div class='container'>
  <div class="row">
    <div id='' class="col s12 m4 l3"> <!-- Note that "m4 l3" was added -->
      <ul>
        <li>
          <%= link_to 'New Client App', new_client_app_path, class: '' %>
        </li>
        <li>
          <%= link_to 'My Account', new_client_app_path, class: '' %>
        </li>
      </ul>
    </div>
    <div id='' class="col s12 m8 l9"> <!-- Note that "m4 l3" was added -->
      <table class='striped'>
        <thead>
          <tr>
            <th>Name</th>
            <th colspan="3">Actions</th>
          </tr>
        </thead>

        <tbody>
          <% @client_apps.each do |client_app| %>
            <tr>
              <td><%= client_app.name %></td>
              <td><%= link_to 'Show', client_app %></td>
              <td><%= link_to 'Destroy', client_app, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>

